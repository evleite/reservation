<assembly>
	<id>reservation-zip</id>
	<formats>
		<format>zip</format>
	</formats>
	<includeBaseDirectory>false</includeBaseDirectory>
	<dependencySets>
		<dependencySet>
			<outputDirectory>/Aircraft Allocation App/deployable
			</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<includes>
				<include>*:ear</include>
			</includes>
		</dependencySet>
		<dependencySet>
			<outputDirectory>/Aircraft Allocation App/javadoc</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<includes>
				<include>*:javadoc</include>
			</includes>
		</dependencySet>
		<dependencySet>
			<outputDirectory>/Aircraft Allocation App/sources</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<includes>
				<include>*:sources</include>
			</includes>
		</dependencySet>
		<dependencySet>
			<outputDirectory>/Aircraft Allocation App/test-sources
			</outputDirectory>
			<unpack>false</unpack>
			<scope>runtime</scope>
			<includes>
				<include>*:test-sources</include>
			</includes>
		</dependencySet>
	</dependencySets>
	<fileSets>
		<fileSet>
			<outputDirectory>/Aircraft Allocation App/documents</outputDirectory>
			<directory>documents</directory>
			<includes>
				<include>*.pdf</include>
			</includes>
		</fileSet>
		<fileSet>
			<outputDirectory>/Aircraft Allocation App/sql</outputDirectory>
			<directory>sql</directory>
		</fileSet>
		<fileSet>
			<outputDirectory>/Aircraft Allocation App/</outputDirectory>
			<directory>..</directory>
			<includes>
				<include>README.md</include>
			</includes>
		</fileSet>
	</fileSets>
</assembly>
